# Git export-ignore patterns

# Build artifacts
*.tar.gz export-ignore
*.pkg.tar.zst export-ignore
dist/ export-ignore
build/ export-ignore

# Development environment
__pycache__/ export-ignore
*.pyc export-ignore
.venv/ export-ignore
node_modules/ export-ignore

# IDE and editor files
.vscode/ export-ignore
.idea/ export-ignore
*.swp export-ignore
*.swo export-ignore
.DS_Store export-ignore

# Project-specific files

# Sensitive files
.env export-ignore
.env.* export-ignore
*.key export-ignore
*.pem export-ignore
